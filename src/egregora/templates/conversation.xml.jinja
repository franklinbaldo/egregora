{% for row in rows %}
<message id="{{ row.msg_id }}" timestamp="{{ row.timestamp }}" author="{{ row.author }}">
  <text>{{ row.text | escape }}</text>
  {% if row.annotations %}
  <annotations>
    {% for annotation in row.annotations %}
    <annotation id="{{ annotation.id }}" timestamp="{{ annotation.timestamp }}" author="{{ annotation.author }}">
      {{ annotation.commentary | escape }}
    </annotation>
    {% endfor %}
  </annotations>
  {% endif %}
</message>
{% endfor %}
