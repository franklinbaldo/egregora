{
  "include": [
    "**/*"
  ],
  "ignore": {
    "useDefaultPatterns": true,
    "customPatterns": [
      "docs/**",
      "tests/**",
      ".git/**",
      "**/tmp*",
      "**/dist/**",
      "**/site/**",
      "**/*.bundle.md",
      ".venv/**",
      "**/__pycache__/**"
    ]
  },
  "output": {
    "style": "markdown"
  }
}
